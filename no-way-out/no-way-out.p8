function makeb(x,y,t,id)
 g=false
 if t==2 then g=true end
 return {x=x,y=y,t=t,g=g,
         v=true,id=id}
end

function makeg(x,y,t,id)
 return {x=x,y=y,t=t,id=id,
         v=true}
end

function buildlevel()
 lvl.b={}
 lvl.g={}
 lvl.ag={}
 for by=1,lvl.y do
  r=lvl.bw[by]
  for bx=1,lvl.x do
   if r&1==1 then
    add(lvl.b,makeb(lvl.x-bx,by-1,1))
   end
   r=r>>1
  end
 end
 for b_p in all(lvl.bb) do
  for bx=b_p[1],b_p[3] do
   for by=b_p[2],b_p[4] do
    add(lvl.b,makeb(bx,by,2))
   end
  end
 end
 for sb in all(lvl.sb) do
  for bx=sb[1],sb[3] do
   for by=sb[2],sb[4] do
    add(lvl.b,makeb(bx,by,3,sb[5]))
   end
  end
 end
 for si,sw in pairs(lvl.sw) do
  add(lvl.g,makeg(sw[1],sw[2],sw[3],sw[4]))
 end
 for it in all(lvl.it) do
  add(lvl.g,makeg(it[1],it[2],it[3]))
 end
 
end

function loadlevel(l_i,nx,ny)
 add(e,l_i)
 li=l_i
 lvl=level[l_i]
 if not lvl.init then
  lvl.init=true
  buildlevel()
 end
 p={x=nx,y=ny,
    d=0,sp=4,
    grab=false,il=true}
end

function _init()
 c={x=60,y=16}
 it={20}
 itsel=1
 loadlevel(0,2,2)
 --loadlevel(5,2,0)
end

function _update60()
 op={x=p.x,y=p.y}
 nd=p.d
 dp={x=0,y=0}
 np={x=p.x,y=p.y}
 if btn(üÖæÔ∏è) then
  p.grab=true
 else
  p.grab=false
 end
 if btnp(‚ùé) then
  itsel+=1
  if itsel>#it then
   itsel=1
  end
 end
 if btnp(‚¨ÜÔ∏è) then
  dp.y-=1
  nd=1
 end
 if btnp(‚¨áÔ∏è) then
  dp.y+=1
  nd=3
 end
 if btnp(‚¨ÖÔ∏è) then
  dp.x-=1
  nd=2
 end
 if btnp(‚û°Ô∏è) then
  dp.x+=1
  nd=0
 end
 np.x+=dp.x
 np.y+=dp.y
 
 if p.grab then
  gp={x=p.x,y=p.y}
  if p.d==0 then gp.x+=1
  elseif p.d==1 then gp.y-=1
  elseif p.d==2 then gp.x-=1
  elseif p.d==3 then gp.y+=1 end
  p.gb=findb(gp)
 else
  p.d=nd
  p.gb=nil
 end
 
 if validp(np) then
  if p.gb!=nil then
   ngbp={x=p.gb.x+dp.x,
         y=p.gb.y+dp.y}
   if validp(ngbp) and p.gb.g then
    p.x=np.x
    p.y=np.y
    p.gb.x+=dp.x
    p.gb.y+=dp.y
   end
  else
   p.x=np.x
   p.y=np.y
  end
 end
 
 for g in all(lvl.g) do
  local pg=p.x==g.x and p.y==g.y
  if g.t<20 then
   local ong = findb(g) or pg
   lvl.ag[g.id]=ong
  elseif g.v and pg then
   g.v=false
   add(it,g.t)
  end
 end
 
 for bl in all(lvl.b) do
  bl.v=true
  if bl.id then
   if lvl.ag[bl.id] then
    bl.v=false
   end
  end
 end
 
 if p.x!=op.x or p.y!=op.y then
  p.il=false
 end
 
 if not p.il then
  for ex in all(lvl.exit) do
   if p.x==ex.x and
      p.y==ex.y then
    loadlevel(ex.nl,ex.nx,ex.ny)
   end
  end
 end 
end

function validp(np)
 if not inbounds(np,lvl) then
  return false
 end
 if not noblock(np) then
  return false
 end
 return true
end

function findb(bp)
 for bl in all(lvl.b) do
  if bl.x==bp.x and bl.y==bp.y then
   return bl
  end
 end
 return nil
end

function noblock(np)
 for bl in all(lvl.b) do
  if bl.v and
     p.gb!=bl and
     bl.x==np.x and 
     bl.y==np.y then
   return false
  end
 end
 return true
end

function _draw()
 cls()
 rect(0,0,127,127,1)
 print('demo', 7)
 for e_i in all(e) do
  --print(e_i)
 end
 draw_grid()
 draw_level()
 draw_ui()
end

