function inbounds(a,b)
 return a.x>=0 and a.x<b.x and
        a.y>=0 and a.y<b.y
end

function sorti(t)
 print(t[0],11)
 for n=2,#t do
  i=n
  while i>1 do
   t[i],t[i-1]=t[i-1],t[i]
   i-=1
  end
 end
end
