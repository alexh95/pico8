function draw_eqit()
 local gsp=128
 local bsp=nil
 if itsel==1 and p.grab then
  if p.gb==nil then
   gsp=130
  else
   if p.gb.g then
    gsp=134
   else
    gsp=132
   end
   bsp=bt_sp(p.gb)
  end
  if bsp then
   spr(bsp,96,10,1,2)
  end
 end
 if p.itinvalid then
  gsp=132
  print('no power',90,30,8)
 end
 if itsel>1 then
  bsp=it_sp[it[itsel]]
  if itm[it[itsel]] then
   bsp+=16
   gsp=134
  end
  spr(bsp,96,16,1,1)
 end
 spr(gsp,92,12,2,2)
end

