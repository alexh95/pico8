function _update60()

end

function draw_grid()
 for i=0,7,1 do
  for j=0,7,1 do
   x=16+i*8-j*4
   y=16+j*4
   spr(33,x,y)
  end
 end
end

function draw_block()
 spr(16,64,64,1,2)
 spr(16,64+8,64,1,2)
 spr(16,64+16,64,1,2)
 spr(16,64-4,64+4,1,2)
 spr(16,64+4,64+4,1,2)
-- spr(16,64,64-8)
end

function _draw()
 cls()
 rect(0,0,127,127,1)
 draw_grid()
 draw_block()
 spr(4,32,24,1,2)
 print('no-way-out demo', 7)
end
