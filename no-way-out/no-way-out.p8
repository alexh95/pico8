 rectfill(pp.x-20,pp.y-16,
          pp.x-12,pp.y+16,0)
 rectfill(pp.x-20,pp.y+16,
          pp.x+24,pp.y+24,0)
 rectfill(pp.x+16,pp.y-16,
          pp.x+24,pp.y+16,0)
