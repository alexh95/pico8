function draw_ui()
 draw_grab()
 draw_it()
 draw_help()
end

function draw_grab()
 local gsp=128
 local bsp=nil
 if p.grab then
  if p.gb==nil then
   gsp=130
  else
   if p.gb.g then
    gsp=134
   else
    gsp=132
   end
   bsp=bt_sp(p.gb.t)
  end
 end
 spr(gsp,92,12,2,2)
 if bsp then
  spr(bsp,96,10,1,2)
 end
end

it_sp={
 [20]=80,
 [21]=81,
}

function draw_it()
 for i,item in pairs(it) do
  sx=8+i*8
  if itsel==i then ssp=65
  else ssp=64 end
  spr(ssp,sx,104)
  spr(it_sp[item],sx,104)
 end
end

function draw_help()
 if li==0 then
  spr(192,12,12,3,3)
 elseif li==1 then
  spr(195,12,12,2,3)
 elseif li==4 and #it>1 then
  spr(197,12,12,2,2)
 end
end
