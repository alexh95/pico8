function validp(np)
 if not inbounds(np,lvl) then
  return false
 end
 if not noblock(np) then
  return false
 end
 return true
end

function findbl(l,bp)
 for bl in all(l.b) do
  if bl.v and
     bl.x==bp.x and
     bl.y==bp.y then
   return bl
  end
 end
 return nil
end

function findb(bp)
 return findbl(lvl,bp)
end

function noblock(np)
 for bl in all(lvl.b) do
  if bl.v and
     p.gb!=bl and
     bl.x==np.x and 
     bl.y==np.y then
   return false
  end
 end
 return true
end

function _draw()
 cls()
 if not won then
  draw_grid()
  draw_level()
  draw_ui()
 else
  win()
 end
end

