function draw_shadow()
 if lvl.shadow then
  local s = lvl.shadow
  if li==5 then
   vertical_shadow(s)
  else
   center_shadow(s)
  end
 end
end

function vertical_shadow(s)
local py=0
 if light_on() then
  py=4*(p.y+2)
 end
 fillp(‚ñë)
 rectfill(s[1],s[2]+py,s[3],s[4],0)
 fillp(‚ñí)
 rectfill(s[1],s[2]+8+py,s[3],s[4],0)
 fillp()
 rectfill(s[1],s[2]+16+py,s[3],s[4],0)
end

function center_shadow(s)
 local pp
 if light_on() then
  pp=pos(c.x,c.y,p.x,p.y)
 else
  pp=pos(c.x,c.y,s[1],s[2])
 end
