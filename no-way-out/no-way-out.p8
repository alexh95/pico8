function makeb(x,y,t)
 g=false
 if t==2 then g=true end
 return {x=x,y=y,t=t,g=g}
end

function buildlevel()
 lvl.b={}
 for by=1,lvl.y do
  r=lvl.bw[by]
  for bx=1,lvl.x do
   if r&1==1 then
    add(lvl.b,makeb(lvl.x-bx,by-1,1))
   end
   r=r>>1
  end
 end
 for b_p in all(lvl.bb) do
  bp={x=b_p[1],y=b_p[2]}
  for bx=b_p[1],b_p[3] do
   for by=b_p[2],b_p[4] do
    add(lvl.b,makeb(bx,by,2))
   end
  end
 end
end

function loadlevel(l_i,nx,ny)
 add(e,l_i)
 lvl=level[l_i]
 if not lvl.init then
  lvl.init=true
  buildlevel()
 end
 p={x=nx,y=ny,
    d=0,sp=4,
    grab=false,il=true}
end

function _init()
 c={x=60,y=16}
 e={}
 loadlevel(0,2,2)
 --loadlevel(4,2,2)
end

function _update60()
 moved=false
 op={x=p.x,y=p.y}
 nd=p.d
 dp={x=0,y=0}
 np={x=p.x,y=p.y}
 if btn(🅾️) then
  p.grab=true
 else
  p.grab=false
 end
 if btn(❎) then
  
 end
 if btnp(⬆️) then
  dp.y-=1
  nd=1
 end
 if btnp(⬇️) then
  dp.y+=1
  nd=3
 end
 if btnp(⬅️) then
  dp.x-=1
  nd=2
 end
 if btnp(➡️) then
  dp.x+=1
  nd=0
 end
 np.x+=dp.x
 np.y+=dp.y
 
 if p.grab then
  gp={x=p.x,y=p.y}
  if p.d==0 then gp.x+=1
  elseif p.d==1 then gp.y-=1
  elseif p.d==2 then gp.x-=1
  elseif p.d==3 then gp.y+=1 end
  p.gb=findb(gp)
 else
  p.d=nd
  p.gb=nil
 end
 
 if validp(np) then
  if p.gb!=nil then
   ngbp={x=p.gb.x+dp.x,
         y=p.gb.y+dp.y}
   if validp(ngbp) and p.gb.g then
    p.x=np.x
    p.y=np.y
    p.gb.x+=dp.x
    p.gb.y+=dp.y
    moved=true
   end
  else
   p.x=np.x
   p.y=np.y
   moved=true
  end
 end
 
 if p.x!=op.x or p.y!=op.y then
  p.il=false
 end
 
 if not p.il then
  for ex in all(lvl.exit) do
   if p.x==ex.x and
      p.y==ex.y then
    loadlevel(ex.nl,ex.nx,ex.ny)
   end
  end
 end 
end

function validp(np)
 if not inbounds(np,lvl) then
  return false
 end
 if not noblock(np) then
  return false
 end
 return true
end

function findb(bp)
 for bl in all(lvl.b) do
  if bl.x==bp.x and bl.y==bp.y then
   return bl
  end
 end
 return nil
end

function noblock(np)
 for bl in all(lvl.b) do
  if p.gb!=bl and
     bl.x==np.x and 
     bl.y==np.y then
   return false
  end
 end
 return true
end

function pos(px,py,tx,ty)
 return {x=px+tx*4-ty*4,
         y=py+tx*4+ty*4}
end

function posb(px,py,tx,ty)
 return pos(px,py,tx-1,ty-1)
end

function draw_grid()
 for i=0,lvl.x-1 do
  for j=0,lvl.y-1 do
   sp=pos(c.x,c.y,i,j)
   spr(33,sp.x,sp.y)
  end
 end
 
 for ex in all(lvl.exit) do
  ep=pos(c.x,c.y,ex.x,ex.y)
  spr(49,ep.x,ep.y)
 end
end

function draw_player()
 pp=posb(c.x,c.y,p.x,p.y)
 if p.d==0 then
  psp=36
 elseif p.d==1 then
  psp=4
 elseif p.d==2 then
  psp=5
 elseif p.d==3 then
  psp=37
 end
 if p.grab then
  psp+=2
 end
 spr(psp,pp.x,pp.y,1,2)
end

function draw_level()
 bo={}
 for bl in all(lvl.b) do
  index=bl.x+bl.y
  if bo[index]==nil then
   bo[index]={}
  end
  add(bo[index],bl)
 end

 for i=0,lvl.x+lvl.y do
  bol=bo[i]
  if bol!=nil then
   for bl_i,bl in pairs(bol) do
    bp=posb(c.x,c.y,bl.x,bl.y)
    bsp=16
    if bl.t==1 then bsp=46
    elseif bl.t==2 then bsp=15 end
    spr(bsp,bp.x,bp.y,1,2)
   end
  end
  if i==p.x+p.y then
   draw_player()
  end
 end
 
end

function _draw()
 cls()
 rect(0,0,127,127,1)
 print('demo', 7)
 for e_i in all(e) do
  --print(e_i)
 end
 draw_grid()
 draw_level()
end
