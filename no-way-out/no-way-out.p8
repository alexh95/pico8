function draw_it()
 for i,item in pairs(it) do
  sx=(i-1)*8
  if itsel==i then ssp=65
  else ssp=64 end
  spr(ssp,sx,104)
  local isp=it_sp[item]
  if itm[item] then
   isp+=16
  end
  spr(isp,sx,104)
 end
 if pwr then
  spr(113,8,112)
 end
end

function draw_map()
 if p.map>0 then
  local mapx=(p.map-1)*64
  rectfill(58,58,124,124,0)
  rect(58,58,124,124,1)
  for i=59,123 do
   tline(59,i,123,i,0,
         (mapx+i-59)/32,1/32)
  end
 end
end

function draw_help()
 if li==0 then
  spr(192,12,12,3,3)
 elseif li==1 then
  spr(195,12,12,2,2)
  spr(243,4,32,4,1)
 elseif li==4 and #it>1 then
  spr(211,92,30,2,1)
  spr(227,92,4,3,1)
 end
end
