-- util
-- useful

function inbounds(a,b)
 return a.x>=0 and a.x<b.x and
        a.y>=0 and a.y<b.y
end

function pos(px,py,tx,ty)
 return {x=px+tx*4-ty*4,
         y=py+tx*4+ty*4}
end

function posb(px,py,tx,ty)
 return pos(px,py,tx-1,ty-1)
end

function update_rgb()
 local rb=findbid('r')
 local gb=findbid('g')
 local bb=findbid('b')
 local rs=findgswid('sr')
 local gs=findgswid('sg')
 local bs=findgswid('sb')
 
 local r=peq(rb,rs)
 local g=peq(gb,gs)
 local b=peq(bb,bs)
  
 lvl.ag['rgb']=r and g and b
end

function findbid(id)
 for b in all(lvl.b) do
  if b.id==id then
   return b
  end
 end
 return nil
end

function findgswid(id)
 for g in all(gbl.g) do
  if g.id==id then
   return g
  end
 end
 return nil
end

function peq(a,b)
 if a and b then
  return a.x==b.x and a.y==b.y
 end
 return false
end

function youwon()
 won=true
 
end

function win()
 print('you escaped',7)
end
