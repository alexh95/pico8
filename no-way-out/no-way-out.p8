 rectfill(pp.x-12,pp.y-12,
          pp.x- 8,pp.y+12,0)
 rectfill(pp.x-12,pp.y+12,
          pp.x+16,pp.y+16,0)
 rectfill(pp.x+12,pp.y-12,
          pp.x+16,pp.y+12,0)
 rectfill(pp.x-12,pp.y-16,
          pp.x+16,pp.y-12,0)
